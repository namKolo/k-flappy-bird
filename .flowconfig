
[ignore]
.*/build/.*
.*/test/*
.*/server/*
.*/node_modules/eslint-plugin-jsx-a11y/*

[include]

[libs]

[options]
module.name_mapper='^\(.*\)$' -> '<PROJECT_ROOT>/src/\1'

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

suppress_type=$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
